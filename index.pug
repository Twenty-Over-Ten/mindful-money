    z-index: 3;
    }
    .bridge::before,
    .bridge::after {
    position: absolute;
    content: '';
    width: 60px;
    height: 3px;
    top: 50%;
    opacity: 1;
    z-index: 1;
    background: #FAE0C3;
    }
    .bridge::before {
    left: -30px;
    }
    .rightbridge::before {
    content: none;
    }
    .leftbridge::after {
    content: none;
    }
    .bridge::after {
    right: -30px;
