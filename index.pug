    /*-------Timleine-------*/
    .wrapper {
    max-width: 1000px;
    margin: 0 auto;
    }
    .timeline {
    max-width: 1000px;
    margin: 0 auto;
    position: relative;
    border: solid #E5E4E6;
    border-width: 3px 0 3px 0;
    padding: 60px 0;
    }
    .timeline::after {
    content: "";
    position: absolute;
    left: 50%;
    width: 3px;
    top: 0;
    height: 100%;
    background: #E5E4E6;
    z-index: -2;
    }
    .leftblip,
    .rightblip {
    overflow: visible;
    margin-bottom: 30px;
    padding: 0 40px;
    position: relative;
    }
    .leftblip::after,
    .rightblip::after {
    content: "";
    position: absolute;
    width: 10px;
    background: #4E8DCE;
    height: 10px;
    right: 0;
    top: 10px;
    border-radius: 50%;
    box-shadow: 0 0 0 4pt #caddf0;
    }
    .rightblip::after {
    left: 0;
    }
    .leftblip::before,
    .rightblip::before {
    content: "";
    position: absolute;
    width: 25px;
    height: 3px;
    background: #E5E4E6;
    right: -25px;
    top: 15px;
    }
    .rightblip::before {
    left: -23px;
    }
